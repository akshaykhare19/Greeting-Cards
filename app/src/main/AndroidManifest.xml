<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.akshay_apps.wishingcards">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/envelope"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WishingCards">
        <activity android:name=".DiwaliActivity"></activity>
        <activity android:name=".NewYearActivity" />
        <activity android:name=".AnniversaryActivity" />
        <activity android:name=".BirthdayActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>